// *Good testcase*
// nested loops with a break
// Should compile
// Expected result: 15

int main() {
    int i = 0, j;
    while ((i < 10)) {
        i = (i + 1);
        j = 0;
        while ((j < 10)) {
            j = (j + 1);
            if ((j = 5)) {
                break;
            } else;
        }
    }
    return (i + j);
}