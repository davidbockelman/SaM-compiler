// triple nested loops with break
// Should compile
// Expected result: 25

int main() {
    int i = 0, j, k;
    while ((i < 10)) {
        i = (i + 1);
        j = 0;
        while ((j < 10)) {
            j = (j + 1);
            k = 0;
            while ((k < 10)) {
                k = (k + 1);
                if ((k = 5)) {
                    break;
                } else;
            }
        }
    }
    return ((i + j) + k);
}